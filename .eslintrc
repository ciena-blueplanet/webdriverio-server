{
    "ecmaFeatures": {
        "arrowFunctions": true,
        "binaryLiterals": true,
        "blockBindings": true,
        "defaultParams": true,
        "forOf": true,
        "generators": true,
        "globalReturn": true,
        "jsx": true,
        "objectLiteralComputedProperties": true,
        "objectLiteralDuplicateProperties": true,
        "objectLiteralShorthandMethods": true,
        "objectLiteralShorthandProperties": true,
        "octalLiterals": true,
        "regexUFlag": true,
        "regexYFlag": true,
        "superInFunctions": true,
        "templateStrings": true,
        "unicodeCodePointEscapes": true
    },
    "env": {
        "amd": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "brace-style": [
            2,
            "1tbs",
            { "allowSingleLine": false }
        ],
        "camelcase": 2,
        "consistent-return": 2,
        "consistent-this": [2, "self"],
        "complexity": [2, 5],
        "curly": [2, "all"],
        "dot-notation": 0,
        "eqeqeq": [2, "allow-null"],
        "guard-for-in": 2,
        "indent": [2, "spaces", 4],
        "key-spacing": [2, {
            "beforeColon": false,
            "afterColon": true
        }],
        "max-depth": [2, 3],
        "max-len": [2, 120],
        "max-params": [2, 5],
        "max-nested-callbacks": [2, 3],
        "max-statements": [2, 50],
        "new-cap": 2,
        "no-bitwise": 2,
        "no-caller": 2,
        "no-comma-dangle": 0,
        "no-empty": 2,
        "no-extend-native": 2,
        "no-floating-decimal": 2,
        "no-lone-blocks": 2,
        "no-loop-func": 2,
        "no-multi-str": 2,
        "no-native-reassign": 2,
        "no-new": 2,
        "no-new-wrappers": 2,
        "no-space-before-semi": 2,
        "no-trailing-spaces": 2,
        "no-unused-vars": [2, {"vars": "all", "args": "after-used"}],
        "no-undef": 2,
        "no-underscore-dangle": 0,
        "no-use-before-define": 2,
        "quotes": [2, "single"],
        "strict": 2,
        "space-after-keywords": [2, "always"],
        "space-before-function-parentheses": [
            2,
            {
                "anonymous": "always",
                "named": "never"
            }
        ],
        "space-in-brackets": [2, "never"],
        "space-infix-ops": 2,
        "space-unary-ops": 2,
        "valid-jsdoc": [
            2,
            {
                "prefer": {
                    "virtual": "abstract",
                    "extends": "augments",
                    "constructor": "class",
                    "const": "constant",
                    "defaultvalue": "default",
                    "desc": "description",
                    "host": "external",
                    "fileoverview": "file",
                    "overview": "file",
                    "emits": "fires",
                    "func": "function",
                    "method": "function",
                    "var": "member",
                    "arg": "param",
                    "argument": "param",
                    "return": "returns",
                    "exception": "throws"
                },
                "requireReturn": false
            }
        ],
        "wrap-iife": [2, "outside"]
    },
    "settings": {
        "ecmascript": 6,
        "jsx": true
    }
}
