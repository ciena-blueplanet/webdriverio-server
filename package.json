{
  "name": "webdriverio-server",
  "version": "1.0.1",
  "description": "Server for running webdriverio tests",
  "config": {
    "coverage_opts": "-x **/spec/** --report cobertura --report lcov --dir spec"
  },
  "scripts": {
    "start": "./bin/www.js",
    "lint": "eslint .",
    "jasmine": "JASMINE_CONFIG_PATH=spec/jasmine.json istanbul cover $npm_config_coverage_opts jasmine",
    "test": "npm run lint && npm run jasmine"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ciena-blueplanet/webdriverio-server.git"
  },
  "keywords": [
    "e2e",
    "testing",
    "webdriverio",
    "selenium"
  ],
  "author": "Peter Banka",
  "contributors": [
    "Adam Meadows <adam.meadows@gmail.com> (https://github.com/job13er)"
  ],
  "engines": {
    "node": ">=5.0.0"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ciena-blueplanet/webdriverio-server/issues"
  },
  "homepage": "https://github.com/ciena-blueplanet/webdriverio-server",
  "devDependencies": {
    "eslint": "^1.10.3",
    "eslint-config-frost-standard": "^0.2.5",
    "istanbul": "^0.4.2",
    "jasmine": "^2.4.1"
  },
  "dependencies": {
    "body-parser": "~1.12.0",
    "cookie-parser": "~1.3.4",
    "debug": "~2.1.1",
    "express": "~4.12.2",
    "jade": "~1.9.2",
    "morgan": "~1.5.1",
    "multer": "^0.1.8",
    "serve-favicon": "~2.2.0"
  },
  "preferGlobal": true,
  "bin": {
    "webdriverio-server": "./bin/www.js",
    "webdriverio-server-init": "./bin/init.sh"
  }
}
